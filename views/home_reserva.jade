extends layout

block content
	h1.
		Cadastros
	ul
		each cadastro, j in cadastros
			form(class='form-signin', action='/home/reserva', method='POST')
				input(type='hidden', name='nome', value=cadastro.name)
				input(type='hidden',name='email', value=cadastro.email)
				input(type='hidden',name='cpf', value=cadastro.cpf)
				input(type='hidden',name='checkin', value=cadastro.dateIn)
				input(type='hidden',name='checkout', value=cadastro.dateOut)
				input(type='hidden',name='cadastro', value=cadastro)
				button(class='btn btn-lg btn-primary btn-block', type='submit') #{cadastro.name} #{cadastro.dateIn} #{cadastro.dateOut}
				span.clearfix