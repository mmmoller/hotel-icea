extends layout

block content
	div.container
		div.row
			div.col-sm-6.col-md-4.col-md-offset-4
				div.home-wall-1
					h1 Quadro de manutenções
	div.container#register-table-wrapper
		each estado in estado_leitos
			p #{estado.cod_leito} : #{estado.limpeza}, #{estado.ocupabilidade}

	div.container
		div.row
			div.col-sm-6.col-md-4.col-md-offset-4
				div.voltar-wall
					form(class='form-signup', action='/home', method='GET')
						button(class='btn btn-lg btn-primary btn-block', type='submit') Voltar
						span.clearfix
