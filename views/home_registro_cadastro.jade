extends layout

block content
	div.container
		div.row
			center
				div.section-wall
					h1.section-title Dados de cadastro
	div.container#folha-ce-table-wrapper
		center
			table#folha-ce-table.table-striped.table-bordered
				tbody
					tr
						td(style='width:30%') Nome
						td #{cadastro.name}
					tr
						td Nome de guerra
						td #{cadastro.name_guerra}
					tr
						td SARAM
						td #{cadastro.saram}
					tr
						td Identidade
						td #{cadastro.identidade}
					tr
						td Unidade
						td #{cadastro.unidade}
					tr
						td Endereço
						td #{cadastro.endereco}
					tr
						td Telefone
						td #{cadastro.telefone}
					tr
						td E-mail
						td #{cadastro.email}
					tr
						td CPF
						td #{cadastro.cpf}
					tr
						td Data de entrada
						td #{cadastro.dateIn}
					tr
						td Data de saída
						td #{cadastro.dateOut}
					tr
						td Número de acompanhantes
						td #{cadastro.acompanhante}
					tr
						td Posto/Graduação
						td #{cadastro.posto}
					tr
						td Nome do curso ou missão
						td #{cadastro.curso}
					tr
						td Solicitante
						td #{cadastro.solicitante}

		